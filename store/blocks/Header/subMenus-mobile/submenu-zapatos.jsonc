            "blockClass":["menu-lv1","zapatos"],
            "itemProps": {
            "type": "internal",
            "noFollow": true,
            "tagTitle": "Zapatos",
            "text": "Zapatos"
            }
        }
    },
    "menu-item#category-pricipal-zapatosM-2": {
        "props": {
            "id": "menu-item-category--pricipal-zapatos",
            "type": "custom",
            "iconId": null,
            "highlight": false,
            "blockClass":["menu-lv1-cierre"],
            "itemProps": {
            "type": "internal",
            "noFollow": false,
            "tagTitle": "zapatos",
            "text": "zapatos"
            }
        }
    }
